digraph G {
    Top;
    AnyValue;
    Bubble;
    "A | B";
    subgraph cluster_NominalTypes {
        B;
        A;
        label="Nominal Types";
    }
    "A & B";
    Never;
    Invalid;

    { rank = same; AnyValue; Bubble }

    Top      -> AnyValue;
